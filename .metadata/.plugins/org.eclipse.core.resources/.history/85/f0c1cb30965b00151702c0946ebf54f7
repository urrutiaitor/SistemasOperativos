package BufferSincronizado;

import java.util.concurrent.Semaphore;

public class Consumidor extends Thread {

	Buffer buffer;
	
	public Consumidor(Buffer buffer) {
		this.buffer = buffer;
	}

	public void run() {
		
		while (true) {
			
			System.out.println("> " + buffer.getNumero());
			
		}
	}
	
}
