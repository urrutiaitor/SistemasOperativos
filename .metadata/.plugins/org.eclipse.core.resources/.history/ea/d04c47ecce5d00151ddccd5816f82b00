package costureros;

import java.util.ArrayList;
import java.util.concurrent.Semaphore;

public class Cesta {

	int numMaxMangas;
	int numMaxCuerpos;
	
	Semaphore semaforoMangas;
	Semaphore semaforoCuerpos;
	
	ArrayList<Cuerpo> cuerpos = new ArrayList<Cuerpo>();
	crearCuerpos(numMaxCuerpos, cuerpos);
	
	ArrayList<Manga> mangas = new ArrayList<Manga>();
	crearMangas(numMaxMangas, mangas);
	
	
}
