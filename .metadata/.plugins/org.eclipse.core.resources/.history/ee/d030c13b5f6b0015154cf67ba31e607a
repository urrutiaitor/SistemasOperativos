package discoteca;

public class Discoteca {

	boolean abierto;
	int aforo;
	
	public Discoteca(int aforo) {
		super();
		this.aforo = aforo;
	}

	public void entrarChico(){
		
	}
	
	public void entrarChica(){
		
	}

	public void cerrar() {
		abierto = false;
	}

	public boolean isAbierto() {
		return abierto;
	}
	
}
