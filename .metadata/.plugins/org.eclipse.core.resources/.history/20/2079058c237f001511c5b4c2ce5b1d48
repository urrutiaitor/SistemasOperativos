package blancanieves;

public class Main {

	Blancanieves bn;
	Enanito enanitos[];
	Mesa mesa;
	
	final static int numEnanitos = 7;
	final static int numAsientos = 4;
	
	
	public static void main(String[] args) {
		Main main = new Main();
	}
	
	public Main () {
		bn = new Blancanieves(mesa, numEnanitos, numAsientos);
		
		for (int i = 0; i < numEnanitos; i++) {
			enanitos[i] = new Enanito(mesa);
		}
		
		for (int i = 0; i < numEnanitos; i++) {
			enanitos[i].start();
		}
		
		for (int i = 0; i < numEnanitos; i++) {
			enanitos[i].join();
		}
		
		bn.join();
		
	}

}
