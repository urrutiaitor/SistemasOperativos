package productorConsumidor;

import java.util.Random;

public class Consumidor extends Thread {

	Buffer buffer;
	
	public Consumidor(Buffer buffer) {
		this.buffer = buffer;
	}

	public void run() {
		Random random = new Random();
		
		while (true) {
			 
			System.out.print(buffer.getNumero() + "                    ");
			
		}
	}
	
}



